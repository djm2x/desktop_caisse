{
  "name": "disktop-class",
  "version": "0.0.0",
  "main": "./dist/main.js",
  "scripts": {
    "pi": "electron-builder install-app-deps",
    "i-electron": "npm i electron --ia32 -D",
    "i": "npm i sqlite3 && npm i electron-builder && npm run postinstall",
    "mode-developpement": "------------------------------------------------------------------",
    "f": "ng s",
    "b": "set ELECTRON_ENV=development && electron --require ts-node/register ./main.ts",
    "build-watch": "ng build --extractCss --watch --base-href ./",
    "electron:serve": "npm run ets && nodemon --watch ./main.ts --exec \"npm run ets\"",
    "s": "electron ./portable8-win32-x64/resources/app/main.js",
    "test": "npm run electron-prod && electron ./dist/main.js",
    "mode-production": "---------------------------------------------------------------------",
    "angular-dist": "electron --require ts-node/register ./main.ts",
    "angular-prod": "ng build --prod --base-href ./",
    "electron-prod": "tsc  --project ./tsconfig.electron.json",
    "prod3": "npm run angular-prod && npm run electron-prod && electron ./electron/main.js",
    "prod": "npm run angular-prod && npm run electron-prod",
    "prod2": "npm run angular-prod && npm run electron-prod",
    "build": "node --require ts-node/register builderAPI.ts",
    "pkg": "electron-packager electron-js portable5",
    "all": "npm run prod && npm run build",
    "init": "git init && git remote add origin https://github.com/djm2x/desktop_caisse.git",
    "push": "git add . && git commit -m \"changement menu\" && git push -u origin master",
    "t": "echo \"Error: no test specified\" && exit 1"
  },
  "private": true,
  "optionalDependencies": {},
  "dependencies": {
    "sequelize": "^5.8.12",
    "sqlite3": "^5.0.0"
  },
